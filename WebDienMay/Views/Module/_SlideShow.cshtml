@model IEnumerable<WatchStore.Models.MSlider>
<div id="wrapper" style="width: 60%; margin-left: auto; margin-right: auto;">
    <div id="mainSlider" class="nivoSlider">
        @foreach (var item in Model)
        {
            <img src="~/Public/library/slider/@item.Img" alt="@item.Name" title="#htmlcaption" />
        }

    </div>
</div>
